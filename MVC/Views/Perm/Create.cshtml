@model PayCal_MVC.Models.PermViewModel
@{
    ViewData["Title"] = "New Permanent Employee Created";
}
<h2>New Permanent Employee Created</h2>

<div class="cardSingle" id="outputTablePerm">
    <table>
        <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Salary</th>
        <th>Bonus</th>
        </tr>
        <tr>
            <td>@Model.Created.EmployeeID</td>
            <td>@Model.Created.FName @Model.Created.LName</td>
            <td>£@Model.Created.Salaryint</td>
            <td>£@Model.Created.Bonusint</td>
        </tr>
    </table>
</div>

<div class="cardsBackBtn">
    <a class="buttons" id="PermEmployeesBtn" asp-area="" asp-controller="Perm" asp-action="Employees">
        <button id="back_btn">Back</button>
    </a>
</div>