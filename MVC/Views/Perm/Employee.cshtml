@model PayCal_MVC.Models.PermViewModel
@{
    ViewData["Title"] = "Permanent Employee";
}
<h2>Permanent Employee</h2>

<div class="cardSingle" id="outputTablePerm">
    <table>
        <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Salary</th>
        <th>Bonus</th>
        </tr>
        <tr>
            <td>@Model.Employee.EmployeeID</td>
            <td>@Model.Employee.FName @Model.Employee.LName</td>
            <td>£@Model.Employee.Salaryint</td>
            <td>£@Model.Employee.Bonusint</td>
        </tr>
    </table>
</div>

<div class="cardsBackBtn">
    <a class="buttons" id="PermEmployeesBtn" asp-area="" asp-controller="Perm" asp-action="Employees">
        <button id="back_btn">Back</button>
    </a>
</div>