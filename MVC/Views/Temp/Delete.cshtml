@model PayCal_MVC.Models.TempViewModel
@{
    ViewData["Title"] = "Deleted Temporary Employee";
}
<h2>Permanent Employee by ID</h2>

<div class="card" id="outputTablePerm">
    <table>
        <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Day Rate</th>
        <th>Weeks Worked</th>
        </tr>
        <tr>
            <td>@Model.Deleted.EmployeeID</td>
            <td>@Model.Deleted.FName @Model.Deleted.LName</td>
            <td>£@Model.Deleted.DayRateint</td>
            <td>£@Model.Deleted.WeeksWorkedint</td>
        </tr>
    </table>
    <br>
    <code>Employee with ID @Model.DeletedID has been deleted.</code>
</div><br>

<div class="column">
    <div class="cards">
        <a class="buttons" id="TempEmployeesBtn" asp-area="" asp-controller="Temp" asp-action="Employees">
            <button id="back_btn">Back</button>
        </a>
    </div>
</div>