@model PayCal_MVC.Models.TempViewModel
@{
    ViewData["Title"] = "New Temporary Employee Created";
}
<h2>New Temporary Employee Created</h2>

<div class="cardSingle" id="outputTablePerm">
    <table>
        <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Day Rate</th>
        <th>Weeks Worked</th>
        </tr>
        <tr>
            <td>@Model.Created.EmployeeID</td>
            <td>@Model.Created.FName @Model.Created.LName</td>
            <td>£@Model.Created.DayRateint</td>
            <td>@Model.Created.WeeksWorkedint</td>
        </tr>
    </table>
</div>

<div class="cardsBackBtn">
    <a class="buttons" id="TempEmployeesBtn" asp-area="" asp-controller="Temp" asp-action="Employees">
        <button id="back_btn">Back</button>
    </a>
</div>