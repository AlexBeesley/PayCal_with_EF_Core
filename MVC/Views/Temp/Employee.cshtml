@model PayCal_MVC.Models.TempViewModel
@{
    ViewData["Title"] = "Temporary Employee";
}
<h2>Temporary Employee</h2>

<div class="cardSingle" id="outputTablePerm">
    <table>
        <tr>
        <th>ID</th>
        <th>Name</th>
        <th>Day Rate</th>
        <th>Weeks Worked</th>
        </tr>
        <tr>
            <td>@Model.Employee.EmployeeID</td>
            <td>@Model.Employee.FName @Model.Employee.LName</td>
            <td>£@Model.Employee.DayRateint</td>
            <td>@Model.Employee.WeeksWorkedint</td>
        </tr>
    </table>
</div>

<div class="cardsBackBtn">
    <a class="buttons" id="TempEmployeesBtn" asp-area="" asp-controller="Temp" asp-action="Employees">
        <button id="back_btn">Back</button>
    </a>
</div>