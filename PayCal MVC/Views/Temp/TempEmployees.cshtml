@{
    ViewData["Title"] = "Temporary";
}
<h2>Temporary Employee Management</h2>
<div class="card">
    <code style="white-space: pre-line">@ViewData["tempList"]</code><br>
</div>

<div class="column">
    <div class="cards">
        <button class="menubtn" id="GetEmployees_btn" onclick="getEmployees()">Get Employee</button>
    </div>
</div>

<div class="column">
    <div class="cards">
        <button class="menubtn" id="payCalculator_btn" onclick="payCalculator()">Pay Calculator</button>
    </div>
</div>

<div class="column">
    <div class="cards">
        <button class="menubtn" id="addEmployee_btn" onclick="addEmployee_getinfo()">Add Employee</button>
    </div>
</div>

<div class="column">
    <div class="cards">
        <button class="menubtn" id="addEmployee_btn" onclick="EditEmployee_getinfo()">Edit Employee</button>
    </div>
</div>

<div class="column">
    <div class="cards">
        <button class="menubtn" id="Refresh_btn" onclick="Refresh()">Refresh</button>
    </div>
</div>