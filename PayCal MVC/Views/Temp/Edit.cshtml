@model PayCal_MVC.Models.TempViewModel
@{
    ViewData["Title"] = "Updated Temporary Employee";
}
<h2>Permanent Employee by ID</h2>
<div class="card">
    <br><code>ID: @Model.Updated.EmployeeID Name: @Model.Updated.FName @Model.Updated.LName Day rate: @Model.Updated.DayRateint Weeks Worked: @Model.Updated.WeeksWorkedint</code><br>
</div>

<div class="column">
    <div class="cards">
        <a class="buttons" id="TempEmployeesBtn" asp-area="" asp-controller="Temp" asp-action="Employees">
            <button id="back_btn">Back</button>
        </a>
    </div>
</div>
