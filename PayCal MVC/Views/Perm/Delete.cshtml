@model PayCal_MVC.Models.PermViewModel
@{
    ViewData["Title"] = "Deleted Permanent Employee";
}
<h2>Permanent Employee by ID</h2>
<div class="card">
    <br><code>ID: @Model.Deleted.EmployeeID Name: @Model.Deleted.FName @Model.Deleted.LName Salary: @Model.Deleted.Salaryint Bonus: @Model.Deleted.Bonusint</code>
    <code>Employee with ID @Model.DeletedID has been deleted.</code><br>
</div>

<div class="column">
    <div class="cards">
        <a class="buttons" id="TempEmployeesBtn" asp-area="" asp-controller="Perm" asp-action="Employees">
            <button id="back_btn">Back</button>
        </a>
    </div>
</div>