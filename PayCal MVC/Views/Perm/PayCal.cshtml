@model PayCal_MVC.Models.PermViewModel
@{
    ViewData["Title"] = "Permanent Employee";
}
<h2>Permanent Employee by ID</h2>
<div class="card">
    <br><code>ID: @Model.PayCalDetails.EmployeeID Name: @Model.PayCalDetails.FName @Model.PayCalDetails.LName Salary: @Model.PayCalDetails.Salaryint Bonus: @Model.PayCalDetails.Bonusint</code>
    <code>Total annual pay after tax: £@Model.PayCalculated</code><br>
</div>

<div class="column">
    <div class="cards">
        <a class="buttons" id="PermEmployeesBtn" asp-area="" asp-controller="Perm" asp-action="Employees">
            <button id="back_btn">Back</button>
        </a>
    </div>
</div>