@model PayCal_MVC.Models.PermViewModel
@{
    ViewData["Title"] = "Updated Permanent Employee";
}
<h2>Permanent Employee by ID</h2>
<div class="card">
    <br><code>ID: @Model.Updated.EmployeeID Name: @Model.Updated.FName @Model.Updated.LName Salary: @Model.Updated.Salaryint Bonus: @Model.Updated.Bonusint</code><br>
</div>

<div class="column">
    <div class="cards">
        <a class="buttons" id="PermEmployeesBtn" asp-area="" asp-controller="Perm" asp-action="Employees">
            <button id="back_btn">Back</button>
        </a>
    </div>
</div>
